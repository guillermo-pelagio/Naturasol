@section scripts
{
    <script>

        //LISTADO DE OF'S
                    tablaOF = $("#tablaOF").DataTable({
                        paging: true,
                        ordering: false,
                        info: true,
                        lengthChange: false,
                        pageLength: 25,
                        "language": {
                            "url": "//cdn.datatables.net/plug-ins/9dcbecd42ad/i18n/Spanish.json"
                        },

                        "ajax": {
                            url: '@Url.Action("obtener_OF", "produccion")',
                            type: "GET",
                            dataType: "json",
                        },
                        "columns": [
                            {
                                "defaultContent": "<button type='button' id='btn-resumen-of' class='btn btn-info' style='padding: 0.5rem; border-radius: 5px;'><i class='fa fa-book' aria-hidden='true'></i></button>"
                                , orderable: false
                                , searchable: false
                            },
                            { data: 'DocNum'  } ,
                            {
                                data: 'Type', "render": function (estatus) {
                                    if (estatus == 'S') {
                                        return '<span class="badge badge-success">Estandar</span>'
                                    }
                                    else if (estatus == 'D') {
                                        return '<span class="badge badge-danger">Desmonte</span>'
                                    }
                                    else if (estatus == 'P') {
                                        return '<span class="badge badge-warning">Especial</span>'
                                    }
                                }
                            },
                            { data: 'ItemCode' },
                            { data: 'PlannedQty' },
                            { data: 'ReleasedQty' },
                            { data: 'PFinalizado' },
                            {
                                data: 'Status', "render": function (estatus) {
                                    if (estatus == 'P') {
                                        return '<span class="badge badge-secondary">Planificado</span>'
                                    }
                                    else if (estatus == 'R') {
                                        return '<span class="badge badge-success">Liberado</span>'
                                    }
                                    else if (estatus == 'C') {
                                        return '<span class="badge badge-danger">Cancelado</span>'
                                    }
                                    else {
                                        return '<span class="badge badge-primary">Finalizado</span>'
                                    }
                                }
                            }
                        ],
                        "createdRow": function (row, data, dataIndex) {
                            if (data.PFinalizado > 90) {
                                $('td', row).css("background-color", "#a8fbc9");
                            }
                        },
                    });

        $("#tablaOF").on("click", '#btn-resumen-of', function () {
            var filaseleccionada = $(this).closest("tr");
            var datos = tablaOF.row(filaseleccionada).data();
            modalDetalleOF(datos);
        });

        function modalDetalleOF(datos) {
            jQuery.ajax({
                url: '@Url.Action("validar_sesion", "Inicio")',
                type: "POST",
                dataType: "json",
                success: function (resultado) {
                    $('#overlay').css('display', 'none');
                    if (resultado == 1) {

                        $("#numeroOFEdit").val(datos.DocNum);
                        $("#articuloOFEdit").val(datos.ItemCode);
                        $("#cantidadPlanificadaEdit").val(datos.PlannedQty);
                        $("#cantidadCompletadaEdit").val(datos.ReleasedQty);
                        $("#costoMPEdit").val(datos.Consumos);
                        $("#costoPTEdit").val(datos.Liberaciones);
                        $("#desviacionEdit").val(datos.Desviacion);
                        $("#comentariosOFEdit").val(datos.Comments);

                        $("#modalDetalleOF").modal("show");
                    }
                    else {
                        alert('Sesion caducada');
                        window.location.reload(false);
                    }
                },
                error: function () {
                    $('#overlay').css('display', 'none');
                    swal("", "Sesión expirada!", "error")
                        .then((value) => {
                            window.location.reload(false);
                        });
                },
            });
        }

        function consultarCostoMP()
                            {
            jQuery.ajax({
                url: '@Url.Action("validar_sesion", "Inicio")',
                type: "POST",
                dataType: "json",
                success: function (resultado) {
                    $('#overlay').css('display', 'none');
                    if (resultado == 1) {
                        var busqueda = {
                            DocNum: $("#numeroOFEdit").val().substring(0, 12),
                        }

                        jQuery.ajax({
                            url: '@Url.Action("contabilizacion_stocks_mp", "produccion")',
                            type: "POST",
                            data: busqueda,
                            success: function (resultado) {
                                window.open('/produccion/contabilizacion_stocks/', "_blank");
                            },
                            error: function (xhr, status, error) {
                            },
                        })

                    }
                    else {
                        alert('Sesion caducada');
                        window.location.reload(false);
                    }
                },
                error: function () {
                    $('#overlay').css('display', 'none');
                    swal("", "Sesión expirada!", "error")
                        .then((value) => {
                            window.location.reload(false);
                        });
                },
            });
        }

        function consultarCostoPT()
                            {
            jQuery.ajax({
                url: '@Url.Action("validar_sesion", "Inicio")',
                type: "POST",
                dataType: "json",
                success: function (resultado) {
                    $('#overlay').css('display', 'none');
                    if (resultado == 1) {
                        var busqueda = {
                            DocNum: $("#numeroOFEdit").val().substring(0, 12),
                        }

                        jQuery.ajax({
                            url: '@Url.Action("contabilizacion_stocks_pt", "produccion")',
                            type: "POST",
                            data: busqueda,
                            success: function (resultado) {
                                window.open('/produccion/contabilizacion_stocks/', "_blank");
                            },
                            error: function (xhr, status, error) {
                            },
                        })

                    }
                    else {
                        alert('Sesion caducada');
                        window.location.reload(false);
                    }
                },
                error: function () {
                    $('#overlay').css('display', 'none');
                    swal("", "Sesión expirada!", "error")
                        .then((value) => {
                            window.location.reload(false);
                        });
                },
            });
                    }
                            /*-----------------------------------------------------------------------------------------*/

                            tablaConsumos = $("#tablaConsumos").DataTable({
                        paging: true,
                                ordering: false,
                        info: true,
                        lengthChange: false,
