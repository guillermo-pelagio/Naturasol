                <div class="form-group row">
                    <label for="nombreTicketEdit" class="col-sm-4 col-form-label"># OF:</label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control" id="numeroOFEdit" readonly>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="correoTicketEdit" class="col-sm-4 col-form-label">Artículo:</label>
                    <div class="col-sm-8">
                        <input type="email" class="form-control" id="articuloOFEdit" readonly>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="departamentoTicketEdit" class="col-sm-4 col-form-label">Cantidad planificada:</label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control" id="cantidadPlanificadaEdit" readonly>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="departamentoTicketEdit" class="col-sm-4 col-form-label">Cantidad completada:</label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control" id="cantidadCompletadaEdit" readonly>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="departamentoTicketEdit" class="col-sm-4 col-form-label">Costo de MP:</label>
                    <div class="col-sm-5">
                        <input type="text" class="form-control" id="costoMPEdit" readonly>
                    </div>
                    <div class="col-sm-3">
                        <button type='button' id='btn-mp-of' class='btn btn-info' style='padding: 0.5rem; border-radius: 5px;' onclick="consultarCostoMP()"><i class="fa-solid fa-dollar-sign" aria-hidden='true'></i></button>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="finalizacionTicketEdit" class="col-sm-4 col-form-label">Costo de PT:</label>
                    <div class="col-sm-5">
                        <input type="text" class="form-control" id="costoPTEdit" readonly>
                    </div>
                    <div class="col-sm-3">
                        <button type='button' id='btn-pt-of' class='btn btn-info' style='padding: 0.5rem; border-radius: 5px;' onclick="consultarCostoPT()"><i class="fa-solid fa-dollar-sign" aria-hidden='true'></i></button>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="finalizacionTicketEdit" class="col-sm-4 col-form-label">Desviacion total:</label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control" id="desviacionEdit" readonly>
                    </div>
                </div>
                <div class="form-group row">
                    <label for="finalizacionTicketEdit" class="col-sm-4 col-form-label">Comentarios:</label>
                    <div class="col-sm-8">
                        <textarea class="form-control" id="comentariosOFEdit" rows="5" readonly></textarea>
                    </div>
                </div>
                <br />
                <div class="modal-footer">
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cerrar</button>
                </div>

            </div>
        </div>
